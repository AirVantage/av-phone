<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <EditTextPreference
        android:defaultValue="@string/pref_password_default"
        android:inputType="textPassword"
        android:key="pref_password_key"
        android:summary=""
        android:title="@string/pref_password" />

    <ListPreference
        android:defaultValue="@string/pref_period_default"
        android:entries="@array/periodentries"
        android:entryValues="@array/periodvalues"
        android:key="pref_period_key"
        android:summary=""
        android:title="@string/pref_period" />

</PreferenceScreen>